<config>
	<menu>
		<saveAsImage>存储为图片</saveAsImage>
		<about>关于</about>
		<version>版本</version>
	</menu>
	<!------------------------------------------------------公共样式----------------------------------------------------------------------------->


	<template>

		 <!-------------------字体样式----------------------->

        <format id='FontFormat' font='微软雅黑' size='12' color='${color}'/>


        <!---------------------效果------------------------------>

        <effects id='Series'>
			<shadow color='${color}{adjustColor:0.5}' alpha='0.3' distance='2' blur='2'/>
		</effects>
		<effects id='Point'>
			<shadow color='${color}' distance='2' angle='45' blur='2' alpha='0.2'/>
		</effects>
		<cover id='Point'>
			<border color='${color}{adjustColor:1.6}' thikness='0.1' alpha='0.8'/>
		</cover>

		<style id='HighLight'>
			<border color='${color}{adjustColor:0.6}' thikness='0.1' alpha='0.8' pixelHinting='false'/>
			<cover>
				<border pixelHinting='true' color='${color}{adjustColor:1.8},${color}{adjustColor:1.4}' radioes='0, 150' angle='90' alpha='1,0' thikness='0.1'/>
				<fill radioes='0,100' alpha='0.5,0' angle='90'
				color='${color}{adjustColor:1.5},${color}{adjustColor:1}'/>
			</cover>
		</style>




        <!----------------------趋势图样式--------------------------------->

		<line id='Simple'>
			<states>
				<normal>
					<border color='${color}' thikness='2' alpha='1'/>
				</normal>
				<hover>
					<border color='${color}{adjustColor:1.3}' thikness='2' alpha='1'/>
				</hover>
				<down>
					<border color='${color}' thikness='2' alpha='1'/>
				</down>
			</states>
		</line>


		<states id='ClassicLine'>
			<normal effects='Series'>
				<border color='${color}' thikness='3' alpha='0.8'/>
				<cover>
					<border pixelHinting='false' color='${color}{adjustColor:1.8}' thikness='1.5' alpha='0.8'/>
				</cover>
			</normal>
			<hover effects='Series'>
				<border color='${color}{adjustColor:1.5}' thikness='4' alpha='0.8'/>
				<cover>
					<border pixelHinting='false' color='${color}{adjustColor:1.8}' thikness='2' alpha='0.8'/>
				</cover>
			</hover>
			<down effects='Series'>
				<border color='${color}{adjustColor:1}' thikness='4' alpha='0.8'/>
				<cover>
					<border pixelHinting='false' color='${color}{adjustColor:1.5}' thikness='2' alpha='0.8'/>
				</cover>
			</down>
		</states>

		<line id='Classic' states='ClassicLine'>
		</line>



		<!----------------------------区域图样式--------------------------------------------------------------------->


		<area id='Simple'>
			<states>
				<normal>
					<border color='${color}' thikness='2' alpha='1'/>
					<fill alpha='0.2' color='${color}'/> 
				</normal>
				<hover>
					<border color='${color}' thikness='2' alpha='1'/>
					<fill alpha='0.4' color='${color}'/>
				</hover>
				<down>
					<border color='${color}' thikness='2' alpha='1'/>
					<fill alpha='0.5' color='${color}'/>
				</down>
			</states>
		</area>

		<area id='Classic' states='ClassicLine'>
			<states>
				<normal>
					<fill radioes='0,255' alpha='0.4,0.2' angle='90'
					color='${color}{adjustColor:1.3},${color}{adjustColor:1.5}'/>
				</normal>
				<hover>
					<fill radioes='0,255' alpha='0.8,0.5' angle='90'
					color='${color}{adjustColor:1.5},${color}{adjustColor:1}'/>
				</hover>
				<down>
					<fill radioes='0,255' alpha='0.8,0.8' angle='90'
					color='${color}{adjustColor:1.2},${color}{adjustColor:1}'/>
				</down>
			</states>
		</area>

	   <!----------------------------柱状图样式--------------------------------------------------------------------->

	  	<states id='SimpleShape'>
			<normal>
				<fill color='${color}' alpha='1'/>
			</normal>
			<hover>
				<fill color='${color}{adjustColor:1.2}' alpha='1'/>
			</hover>
			<down>
				<fill color='${color}{adjustColor:0.8}' alpha='1'/>
			</down>
		</states>

		<states id='ClassicShape' normal='HighLight' hover='HighLight' down='HighLight'>
			<normal effects='Series'>
				<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45'
				color='${color}{adjustColor:1.5},${color}{adjustColor:1.1},${color}{adjustColor:0.9}'/>
			</normal>
			<hover effects='Series'>
				<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45'
				color='${color}{adjustColor:1.5},${color}{adjustColor:1.3},${color}{adjustColor:0.9}'/>
			</hover>
			<down effects='Series'>
				<fill radioes='0,255' alpha='0.8,0.8' angle='45'
				color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
			</down>
		</states>

		<column id='Simple' states='SimpleShape'/>
		<column id='Classic' states='ClassicShape'/>

		<stackedColumn id='Simple' states='SimpleShape'/>
		<bar id='Simple' states='SimpleShape'/>
		<stackedBar id='Simple' states='SimpleShape'/>


        <!----------------------------数据节点------------------------------------------------------------->
		<dataRender id='Square'>
			<rect>
				<states>
					<normal width='5' height='5'>
						<border color='${color}' thikness='2' alpha='0.8'/>
						<fill color='${color}' alpha='0.8'/>
					</normal>
					<hover width='6' height='6'>
						<border color='${color}{adjustColor:1.3}' thikness='4' alpha='0.8'/>
						<fill color='${color}{adjustColor:1.2}' alpha='0.8'/>
					</hover>
					<down width='5' height='5'>
						<border color='${color}' thikness='4' alpha='0.8'/>
						<fill color='${color}{adjustColor:0.8}' alpha='0.8'/>
					</down>
				</states>
			</rect>
		</dataRender>
		<dataRender id='Diamond'>
			<diamond>
				<states>
					<normal radius='3'>
						<border color='${color}' thikness='2' alpha='0.8'/>
						<fill color='${color}' alpha='0.8'/>
					</normal>
					<hover radius='4'>
						<border color='${color}{adjustColor:1.3}' thikness='4' alpha='0.8'/>
						<fill color='${color}{adjustColor:1.2}' alpha='0.8'/>
					</hover>
					<down radius='5'>
						<border color='${color}' thikness='4' alpha='0.8'/>
						<fill color='${color}{adjustColor:0.8}' alpha='0.8'/>
					</down>
				</states>
			</diamond>
		</dataRender>
		<dataRender id='SimpleBubble'>
			<circle>
				<states>
					<normal>
						<fill color='${color}{adjustColor:1.4}, ${color}' alpha='0.6,0.6' type='radial'/>
					</normal>
					<hover>
						<fill color='${color}{adjustColor:1.5}, ${color}{adjustColor:1.3}' alpha='0.8,0.8' type='radial'/>
					</hover>
					<down>
						<fill color='${color}{adjustColor:1.4}, ${color}' alpha='0.8,0.8' type='radial'/>
					</down>
				</states>
			</circle>
			<decorate>
				<circle offsetRadius='-0.3' offsetX='-0.4' offsetY='0.4'>
					<states>
						<normal >
							<fill angle='90' color='FFFFFF,FFFFFF' alpha='0.6,0' type='radial'/>
						</normal>
						<hover>
							<fill angle='90' color='FFFFFF,FFFFFF' alpha='0.4,0' type='radial'/>
						</hover>
						<down>
							<fill angle='90' color='FFFFFF,FFFFFF' alpha='0.4,0' type='radial'/>
						</down>
					</states>
				</circle>
			</decorate>
		</dataRender>

		<dataRender id='ClassicBubble'>
			<circle>
				<states>
					<normal>
						<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
						<cover>Point</cover>
						<effects>Point</effects>
						<fill color='${color}{adjustColor:1.4}, ${color}' alpha='0.6,0.8' type='radial'/>
					</normal>
					<hover>
						<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
						<cover>Point</cover>
						<effects>Point</effects>
						<fill color='${color}{adjustColor:1.5}, ${color}{adjustColor:1.3}' alpha='0.8,0.8' type='radial'/>
					</hover>
					<down>
						<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
						<cover>Point</cover>
						<effects>Point</effects>
						<fill color='${color}' alpha='0.8,0.8' type='radial'/>
					</down>
				</states>
			</circle>
		</dataRender>

		<dataRender id='SimplePoint'>			
			<circle>
				<states>
					<normal radius='3'>
						<border color='${color}' thikness='1' alpha='1'/>
						<fill color='FFFFFF'/>
					</normal>
					<hover radius='4'>
						<border color='${color}' thikness='2' alpha='1'/>
						<fill color='FFFFFF'/>
					</hover>
					<down radius='4'>
						<border color='${color}' thikness='2' alpha='1'/>
						<fill color='FFFFFF'/>
					</down>
				</states>
			</circle>		
		</dataRender>

		<dataRender id='ClassicPoint'>
			<circle>
				<states>
					<normal radius='3'>
						<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
						<cover>Point</cover>
						<effects>Point</effects>
						<fill color='${color}{adjustColor:1.6}, ${color}' alpha='0.8,0.8' type='radial'/>
					</normal>
					<hover radius='6'>
						<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
						<cover>Point</cover>
						<effects>Point</effects>
						<fill color='${color}{adjustColor:1.6}, ${color}{adjustColor:1.3}' alpha='0.5,0.5' type='radial'/>
					</hover>
					<down radius='6'>
						<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
						<cover>Point</cover>
						<effects>Point</effects>
						<fill color='${color}{adjustColor:1.6}, ${color}{adjustColor:1.3}' alpha='0.5,0.5' type='radial'/>
					</down>
				</states>
			</circle>
		</dataRender>

		

		<!--------------------图例------------------------------------->

		<icon id='Simple'>
			<rect>
				<states>
					<normal width='20' height='12'>
						<fill color='${color}'/>
					</normal>
					<hover width='20' height='12'>
						<fill color='${color}{adjustColor:1.3}'/>
					</hover>
				</states>
			</rect>
		</icon>



		<legend id='Simple' label='${seriesName}' enable='false' hPadding='5' vPadding='0' vMargin='2'>
			<icon>Simple</icon>
			<label format='FontFormat'/>
		</legend>

		<legend id='Classic' label='${seriesName}' enable='false' hPadding='5' vPadding='0' vMargin='2'>
			<icon>Classic</icon>
			<label format='FontFormat'/>
		</legend>


		<!--------------------ToolTip----------------------------------------->

		<tooltip id='Simple' enable='true' locked='false' self="${tooltip}" group="${tooltip}">
			<self format='FontFormat' radius='0' padding='5' vMargin='10'>
				<border thikness='1' alpha='1' color='${color}' pixelHinting='true'/>
				<fill color='FFFFFF' alpha='0.9'/>
				<cover>
					<border color='FFFFFF'/>
				</cover>
			</self>
			<group format='FontFormat' radius='0' padding='5' vMargin='10'>
				<border thikness='1' alpha='1' color='CCCCCC' pixelHinting='true'/>
				<fill color='FFFFFF' alpha='0.9'/>
			</group>
		</tooltip>


        <!--------------------颜色板------------------------------->
        <colors id='Default'>
			<color>097DDA</color>
            <color>728F1C</color>
            <color>C451C8</color>
            <color>CCCC99</color>
            <color>885C88</color>
            <color>FE5B3B</color>
            <color>5B7C92</color>
            <color>AB8718</color>
            <color>9BD124</color>
            <color>634BA7</color>
            <color>017E01</color>
            <color>097DDA</color>
            <color>2F8A93</color>
		</colors>
		<colors id='RGB'>
            <color>#548DCA</color>
            <color>#EF306A</color>
            <color>#98C045</color>
            <color>#FDB52F</color>
            <color>#4AC0C0</color>
            <color>#A085D4</color>
            <color>#BD4D4B</color>
            <color>#959595</color>
            <color>#6DCFF6</color>
            <color>#FBE601</color>
            <color>#438071</color>
            <color>#4E555F</color>
		</colors>


	</template>


	<!---------------------------------------------------------公共样式-------------------------------------------------------------------------->



    <!------------------------------------------------------------------------------------------------------------------------------------------->
	<styles>

		<!-- 默认样式  ： 精简-->
		<Simple animation='true'>
			<title hAlign='center' vMargin='0' vPadding='0' format='FontFormat'>
				<format size='14' color='3C4145' bold='true'/>
			</title>

			<subTitle format='FontFormat' hAlign='center' vMargin='0' vPadding='0'>
				<format color='555555'/>
			</subTitle>

			<chartBG paddingLeft='10' paddingRight='10' paddingTop='10' paddingBottom='10'>
				<border color='E2E2E2'/>
				<fill color='FFFFFF'/>
			</chartBG>

			<gridField>
				<border color='E2E2E2' alpha='1' thikness='1' pixelHinting='true'/>
				<hGrid>
					<border color='E2E2E2' thikness='1'/>
				</hGrid>
				<vGrid>
					<border color='E2E2E2' thikness='1'/>
				</vGrid>
			</gridField>

			<xAxis type="field" labelDisplay="normal">
				<title format='FontFormat'>
					<format color='393939'/>
				</title>
				<tickMark color='E4E2E2' thikness='1' alpha='0'/>
				<label format='FontFormat'>
					<format color='9F9F9F'/>
					<text value='${label}'/>
				</label>
			</xAxis>

			<yAxis type="linear">
				<title format='FontFormat'>
					<format color='393939'/>
				</title>
				<tickMark color='E4E2E2' alpha='0'/>
				<label format='FontFormat'>
					<format color='9F9F9F'/>
					<text value='${label}'/>
				</label>
			</yAxis>

			<tooltip>Simple</tooltip>
			<legend>Simple</legend>

			<valueLabel format='FontFormat' margin='0' padding='5'>
				<text value='${valueLabel}'/>
			</valueLabel>

			<innerValueLabel format='FontFormat'>
				<format color='#EEEEEE'/>
				<text value='${valueLabel}'/>
			</innerValueLabel>

			<!---------------------------- 序列 ------------------------->

			<line style='Simple' labelDisplay="none">
				<dataRender style='SimplePoint'/>
			</line>

			<area style='Simple' labelDisplay="none">
				<dataRender style='SimplePoint'/>
			</area>

			<column style='Simple' maxItemSize='100' labelDisplay="inner">
				<dataRender style='SimplePoint'/>
			</column>

			<stackedColumn style='Simple' labelDisplay="normal">
				<dataRender style='SimplePoint'/>
			</stackedColumn>

			<bar style='Simple' maxItemSize='100' labelDisplay="inner">
				<dataRender style='SimplePoint'/>
			</bar>

			<stackedBar style='Simple' labelDisplay="normal">
				<dataRender enable='false'/>
			</stackedBar>

			<marker labelDisplay="none">
				<dataRender style='SimplePoint'></dataRender>
			</marker>

			<bubble maxRadius='60' minRadius='5' labelDisplay="inner">
				<dataRender style='SimpleBubble'></dataRender>
			</bubble>


			<!--- 模板 ---------------->
			<template>
            </template>

            <!----色系----------------->
			<colors>Default</colors>
		</Simple>










		<!----------------------------------- 默认样式  ： 经典色系-------------------------------------------------------------------------------->
		<Classic animation='true'>
			<title hAlign='center' vMargin='0' vPadding='0' format='FontFormat'>
				<format size='14' color='3C4145' bold='true'/>
			</title>
			<subTitle hAlign='center' vMargin='0' vPadding='0' format='FontFormat'>
				<format color='555555'/>
			</subTitle>
			<chartBG paddingLeft='10' paddingRight='10' paddingTop='10' paddingBottom='10' radius='10'>
				<border color='E2E2E2'/>
				<fill color='FFFFFF'/>
			</chartBG>
			<gridField radius='10'>
				<border color='E2E2E2' alpha='1' thikness='1' pixelHinting='true'/>
				<hGrid>
					<border color='E2E2E2' thikness='1'/>
				</hGrid>
				<vGrid></vGrid>
			</gridField>
			<xAxis type="field" labelDisplay="normal">
				<title format='FontFormat'>
					<format color='393939'/>
				</title>
				<tickMark color='E4E2E2' thikness='1' alpha='0'/>
				<label format='FontFormat'>
					<format color='9F9F9F'/>
					<text value='${label}'/>
				</label>
			</xAxis>
			<yAxis type="linear">
				<title format='FontFormat'>
					<format color='393939'/>
				</title>
				<tickMark color='E4E2E2' alpha='0'/>
				<label format='FontFormat'>
					<format color='9F9F9F'/>
					<text value='${label}'/>
				</label>
			</yAxis>
			<tooltip>Simple</tooltip>
			<legend>Simple</legend>



			<valueLabel margin='0' padding='5' format='FontFormat'>
				<text value='${valueLabel}'/>
			</valueLabel>

			<innerValueLabel format='FontFormat'>
				<format color='${color}{adjustColor:0.6}'/>
				<text value='${valueLabel}'>
					<effects>
						<shadow color='${color}{adjustColor:1.6}' alpha='1' distance='1' blur='1' angle='90'/>
					</effects>
				</text>
			</innerValueLabel>

			<!-- 序列 -->
			<line style='Classic' labelDisplay="none">
				<dataRender style='ClassicPoint'/>
			</line>

			<area style='Classic' labelDisplay="none">
				<dataRender style='ClassicPoint'/>
			</area>

			<column style='Classic' maxItemSize='100' labelDisplay="inner">
				<dataRender style='ClassicPoint'/>
			</column>

			<stackedColumn states='columnStates' labelDisplay="normal">
				<dataRender enable='false'/>
			</stackedColumn>
			
			<bar states='columnStates' maxItemSize='100' labelDisplay="inner">
				<states>
					<normal effects='seriesShadowEffect'>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.1},${color}{adjustColor:1.5}'/>
					</normal>
					<hover effects='seriesShadowEffect'>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.3},${color}{adjustColor:1.5}'/>
					</hover>
					<down effects='seriesShadowEffect'>
						<fill radioes='0,255' alpha='0.8,0.8' angle='-45'
						color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
					</down>
				</states>
			</bar>

			<stackedBar states='columnStates' labelDisplay="normal">
				<dataRender enable='false'/>
				<states>
					<normal>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.1},${color}{adjustColor:1.5}'/>
					</normal>
					<hover>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.3},${color}{adjustColor:1.5}'/>
					</hover>
					<down>
						<fill radioes='0,255' alpha='0.8,0.8' angle='-45'
						color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
					</down>
				</states>
			</stackedBar>
			<marker labelDisplay="none">
				<markerRender>
					<states normal='highLineStyle' hover='highLineStyle' down='highLineStyle'>
						<normal radius='5'>
							<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45' color='${color}{adjustColor:1.2},${color}{adjustColor:1.1},${color}{adjustColor:0.9}'/>
						</normal>
						<hover radius='6'>
							<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45' color='${color}{adjustColor:1.5},${color}{adjustColor:1.3},${color}{adjustColor:0.9}'/>
						</hover>
						<down radius='5'>
							<fill radioes='0,255' alpha='0.8,0.8' angle='45' color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
						</down>
					</states>
				</markerRender>
			</marker>
			<bubble maxRadius='60' minRadius='5' labelDisplay="inner">
				<dataRender style='ClassicBubble'/>
			</bubble>


			<!-- 基础定义 -->
			<template>
				<style id='dataRenderStyle' radius='3'>
					<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
					<cover>
						<border color='${color}{adjustColor:1.6}' thikness='0.1' alpha='0.8'/>
					</cover>
					<effects id='dataRenderEffects'>
						<shadow color='${color}' distance='2' angle='45' blur='2' alpha='0.2'/>
					</effects>
				</style>
				<style id='legendIconStyle' width='15' height='11'>
					<border color='${color}{adjustColor:0.6}' thikness='.5'/>
					<cover>
						<border pixelHinting='true' color='${color}{adjustColor:1.8},${color}{adjustColor:1.4}' radioes='0, 200' angle='90' alpha='1,0' thikness='0.1'/>
					</cover>
				</style>
				
				<style id='highLineStyle'>
					<border color='${color}{adjustColor:0.6}' thikness='0.1' alpha='0.8' pixelHinting='false'/>
					<cover>
						<border pixelHinting='true' color='${color}{adjustColor:1.8},${color}{adjustColor:1.4}' radioes='0, 150' angle='90' alpha='1,0' thikness='0.1'/>
						<fill radioes='0,100' alpha='0.5,0' angle='90'
						color='${color}{adjustColor:1.5},${color}{adjustColor:1}'/>
					</cover>
				</style>
				
				<effects id='seriesShadowEffect'>
					<shadow color='${color}{adjustColor:0.5}' alpha='0.3' distance='2' blur='2'/>
				</effects>
			</template>
			<colors>Default</colors>
		</Classic>











		<!--------------------------------------- 黑色  ----------------------------------------------------------------------->
		<Black animation='true'>
			<title hAlign='center' vMargin='0' vPadding='0'>
				<format size='14' color='ADADAD' bold='true' font='微软雅黑'/>
				<text substr='30'>
					<effects>
						<shadow color='000000' alpha='1' distance='1' blur='1' angle='90'/>
					</effects>
				</text>
			</title>
			<subTitle hAlign='center' vMargin='0' vPadding='0'>
				<text substr='30'/>
				<format size='12' color='ADADAD' bold='false' font='微软雅黑'/>
			</subTitle>
			<chartBG paddingLeft='10' paddingRight='10' paddingTop='10' paddingBottom='10' radius='10'>
				<border color='595959'/>
				<fill color='161616'/>
			</chartBG>
			<gridField radius='10'>
				<border color='0C0C0C' alpha='1' thikness='2' pixelHinting='true'/>
				<fill color='25252A,29292A' alpha='1,1' radioes='0,255'/>
				<hGrid>
					<border color='434348' thikness='1'/>
				</hGrid>
				<vGrid>
					<border color='434348' thikness='1'/>
				</vGrid>
			</gridField>
			<xAxis type="field" labelDisplay="normal">
				<title>
					<format color='ADADAD' font='微软雅黑'/>
				</title>
				<tickMark color='E4E2E2' thikness='1' alpha='0'/>
				<label>
					<format color='626262' font='微软雅黑'/>
					<text value='${label}'>
						<effects>
							<shadow color='000000' alpha='1' distance='1' blur='1' angle='90'/>
						</effects>
					</text>
				</label>
			</xAxis>
			<yAxis type="linear" labelDisplay="normal">
				<title>
					<format color='ADADAD' font='微软雅黑'/>
				</title>
				<tickMark color='E4E2E2' alpha='0'/>
				<label>
					<format color='626262' font='微软雅黑'/>
					<text value='${label}'>
						<effects>
							<shadow color='000000' alpha='1' distance='1' blur='1' angle='90'/>
						</effects>
					</text>
				</label>
			</yAxis>
			<tooltip enable='true' locked='true' self="${tooltip}" group="${tooltip}">
				<self radius='0' padding='5' vMargin='10'>
					<border thikness='1' alpha='1' color='${color}' pixelHinting='true'/>
					<fill color='333333' alpha='0.7'/>
					<format font='微软雅黑' size='12' color='${color}' bold='false'/>
					<text>
						<effects>
							<shadow color='000000' alpha='1' distance='1' blur='1' angle='90'/>
						</effects>
					</text>
				</self>
				<group radius='0' padding='5' vMargin='10'>
					<border thikness='1' alpha='1' color='CCCCCC' pixelHinting='true'/>
					<fill color='333333' alpha='0.7'/>
					<format font='微软雅黑' size='12' color='${color}' bold='false'/>
					<text>
						<effects>
							<shadow color='000000' alpha='1' distance='1' blur='1' angle='90'/>
						</effects>
					</text>
				</group>
			</tooltip>
			<legend label="${seriesName}" enable='false' hPadding='5' vPadding='2' vMargin='2' radius='5'>
				<border color='000000' thikness='1' alpha='0.5'/>
				<fill color='25252A' alpha='.5'/>
				<icon>
					<states normal='legendIconStyle' hover='legendIconStyle'>
						<normal>
							<fill radioes='0,100,255' alpha='1,1,1' angle='45'
							color='${color} {adjustColor : 1.1}, ${color}{adjustColor:1.1} ,${color}{adjustColor:0.9}'/>
						</normal>
						<hover>
							<fill radioes='0,150,255' alpha='1,1,1' angle='45'
							color='${color}{adjustColor : 1.5}, ${color}{adjustColor:1.1} ,${color}{adjustColor:0.9}'/>
						</hover>
					</states>
				</icon>
				<label>
					<format size='12' color='${color}' font='微软雅黑'/>
					<fill color='FF0000' alpha='0'/>
					<text substr='30'>
						<effects>
							<shadow color='000000' alpha='1' distance='1' blur='1' angle='90'/>
						</effects>
					</text>
				</label>
			</legend>
			<dataRender>
				<states normal='dataRenderStyle' hover='dataRenderStyle'>
					<normal>
						<fill color='${color}{adjustColor:1.6}, ${color}' alpha='0.8,0.8' type='radial'/>
					</normal>
					<hover radius='6'>
						<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.5'/>
						<fill color='${color}{adjustColor:1.6}, ${color}{adjustColor:1.3}' alpha='0.3,0.3' type='radial'/>
					</hover>
				</states>
			</dataRender>
			<valueLabel margin='0' padding='5'>
				<format color='${color}' font='微软雅黑'/>
				<text value='${valueLabel}'/>
			</valueLabel>
			<innerValueLabel>
				<format color='${color}{adjustColor:0.5}' font='微软雅黑'/>
				<text value='${valueLabel}'>
					<effects>
						<shadow color='${color}{adjustColor:1.4}' alpha='1' distance='1' blur='1' angle='90'/>
					</effects>
				</text>
			</innerValueLabel>
			<!-- 序列 -->
			<line states='lineStates' labelDisplay="none">
				<states>
					<normal effects='seriesShadowEffect'/>
					<hover effects='seriesShadowEffect'/>
					<down effects='seriesShadowEffect'/>
				</states>
			</line>
			<area states='lineStates' labelDisplay="none">
				<states>
					<normal effects='seriesShadowEffect'>
						<border alpha='0.8'/>
						<fill radioes='0,255' alpha='0.4,0.1' angle='90'
						color='${color}{adjustColor:1.3},${color}{adjustColor:1.5}'/>
					</normal>
					<hover>
						<fill radioes='0,255' alpha='0.6,0.4' angle='90'
						color='${color}{adjustColor:1.5},${color}{adjustColor:1}'/>
					</hover>
					<down>
						<fill radioes='0,255' alpha='0.8,0.5' angle='90'
						color='${color}{adjustColor:1.2},${color}{adjustColor:1}'/>
					</down>
				</states>
			</area>
			<stackedColumn states='columnStates' labelDisplay="normal">
				<dataRender enable='false'/>
			</stackedColumn>
			<column states='columnStates' maxItemSize='100' labelDisplay="inner">
				<states>
					<normal effects='seriesShadowEffect'/>
					<hover effects='seriesShadowEffect'/>
					<down effects='seriesShadowEffect'/>
				</states>
			</column>
			<bar states='columnStates' maxItemSize='100' labelDisplay="inner">
				<states>
					<normal effects='seriesShadowEffect'>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.1},${color}{adjustColor:1.5}'/>
					</normal>
					<hover effects='seriesShadowEffect'>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.3},${color}{adjustColor:1.5}'/>
					</hover>
					<down effects='seriesShadowEffect'>
						<fill radioes='0,255' alpha='0.8,0.8' angle='-45'
						color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
					</down>
				</states>
			</bar>
			<stackedBar states='columnStates' labelDisplay="normal">
				<dataRender enable='false'/>
				<states>
					<normal>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.1},${color}{adjustColor:1.5}'/>
					</normal>
					<hover>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='-45'
						color='${color}{adjustColor:0.9},${color}{adjustColor:1.3},${color}{adjustColor:1.5}'/>
					</hover>
					<down>
						<fill radioes='0,255' alpha='0.8,0.8' angle='-45'
						color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
					</down>
				</states>
			</stackedBar>
			<marker labelDisplay="none">
				<markerRender>
					<states normal='highLineStyle' hover='highLineStyle' down='highLineStyle'>
						<normal radius='5'>
							<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45'
							color='${color}{adjustColor:1.2},${color}{adjustColor:1.1},${color}{adjustColor:0.9}'/>
						</normal>
						<hover radius='6'>
							<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45'
							color='${color}{adjustColor:1.5},${color}{adjustColor:1.3},${color}{adjustColor:0.9}'/>
						</hover>
						<down radius='5'>
							<fill radioes='0,255' alpha='0.8,0.8' angle='45'
							color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
						</down>
					</states>
				</markerRender>
			</marker>
			<bubble maxRadius='60' minRadius='5' labelDisplay="inner">
				<bubbleRender>
					<states normal='dataRenderStyle' hover='dataRenderStyle'>
						<normal>
							<fill color='${color}{adjustColor:1.4}, ${color}' alpha='0.6,0.8' type='radial'/>
						</normal>
						<hover>
							<fill color='${color}{adjustColor:1.5}, ${color}{adjustColor:1.3}' alpha='0.8,0.8' type='radial'/>
						</hover>
					</states>
				</bubbleRender>
			</bubble>
			<!-- 基础定义 -->
			<template>
				<style id='dataRenderStyle' radius='3'>
					<border color='${color}{adjustColor:0.8}' thikness='0.1' alpha='0.8'/>
					<cover>
						<border color='${color}{adjustColor:1.6}' thikness='0.1' alpha='0.8'/>
					</cover>
					<effects id='dataRenderEffects'>
						<shadow color='${color}' distance='2' angle='45' blur='2' alpha='0.2'/>
					</effects>
				</style>
				<style id='legendIconStyle' width='15' height='11'>
					<border color='${color}{adjustColor:0.2}' thikness='1'/>
				</style>
				<states id='columnStates' normal='highLineStyle' hover='highLineStyle' down='highLineStyle'>
					<normal>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45'
						color='${color}{adjustColor:1.5},${color}{adjustColor:1.1},${color}{adjustColor:0.9}'/>
					</normal>
					<hover>
						<fill radioes='0,120,255' alpha='0.8,0.8,0.8' angle='45'
						color='${color}{adjustColor:1.5},${color}{adjustColor:1.3},${color}{adjustColor:0.9}'/>
					</hover>
					<down>
						<fill radioes='0,255' alpha='0.8,0.8' angle='45'
						color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
					</down>
				</states>
				<style id='highLineStyle'>
					<border color='${color}{adjustColor:0.6}' thikness='0.1' alpha='0.8' pixelHinting='false'/>
					<cover>
						<border pixelHinting='true' color='${color}{adjustColor:1.8},${color}{adjustColor:1.4}' radioes='0, 150' angle='90' alpha='1,0' thikness='0.1'/>
						<fill radioes='0,100' alpha='0.5,0' angle='90'
						color='${color}{adjustColor:1.5},${color}{adjustColor:1}'/>
					</cover>
				</style>
				<states id='lineStates'>
					<normal>
						<border color='${color}' thikness='2' alpha='0.8'/>
					</normal>
					<hover>
						<border color='${color}{adjustColor:1.5}' thikness='2' alpha='0.8'/>
					</hover>
					<down>
						<border color='${color}{adjustColor:1}' thikness='2' alpha='0.8'/>
					</down>
				</states>
				<effects id='seriesShadowEffect'>
					<shadow color='${color}{adjustColor:0.2}' alpha='0.5' distance='1' blur='1'/>
				</effects>
			</template>
			<colors>
                <color>00A8FF</color>
                <color>A97601</color>
                <color>FF99FF</color>
                <color>9BD124</color>
                <color>7967B9</color>
                <color>B3B3B3</color>
                <color>CCCC99</color>
                <color>0A9D0B</color>
                <color>0B57B2</color>
                <color>E64826</color>
                <color>9F21C8</color>
                <color>2F8A93</color>
                <color>4B5D84</color>
			</colors>
		</Black>
	</styles>
</config>
