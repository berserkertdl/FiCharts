<theme>
	<menu>
		<saveAsImage>存储为图片</saveAsImage>
		<about>关于</about>
		<version>版本</version>
	</menu>
	
	<styles>
	    <!-- 黑色  -->
	    <black animation='true'>
	        <pieSeries>
	            <states>
	                <normal>
	                    <fill type='radial' radioes='0,230,255' alpha='0.8,0.8,0.8' angle='0'
	                    color='${color}{adjustColor:1},${color}{adjustColor:1.2},${color}{adjustColor:1}'/>
	                    <border thikness='1' alpha='1' color='${color}{adjustColor:0.6}' pixelHinting='false'/>
	                </normal>
	                <hover>
	                    <fill type='radial' radioes='0,220,255' alpha='1,1,1' angle='0'
	                    color='${color}{adjustColor:1},${color}{adjustColor:1.5},${color}{adjustColor:1}'/>
	                     <border thikness='1' alpha='0.8' color='${color}{adjustColor:0.6}' pixelHinting='false'/>
	                </hover>
	                <down>
	                    <fill type='radial' radioes='0,255' alpha='0.8,0.8' angle='0'
	                    color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
	                </down>
	            </states>
	        </pieSeries>
	            
	        <series>
	            <pie labelField='label' valueField='value'/>
	        </series>
	        
	        <title hAlign='center' vMargin='0' vPadding='0'>
	            <format size='14' color='ADADAD' bold='true' font='微软雅黑'/>
                <text substr='30'>
                    <effects>
                        <shadow color='000000' alpha='1' distance='1' blur='1' angle='90'/>
                    </effects>
                </text>
	        </title>
	        <subTitle hAlign='center' vMargin='0' vPadding='0'>
	            <text substr='30'/>
                <format size='12' color='ADADAD' bold='false' font='微软雅黑'/>
	        </subTitle>
	        <chartBG paddingLeft='10' paddingRight='10' paddingTop='10' paddingBottom='10' radius='10' gutterRight='10'>
	            <border color='595959'/>
                <fill color='25252A'/>
	        </chartBG>
	        
	        <tooltip enable='true' locked='false' radius='0' padding='5' vMargin='10'
	        	text='${xLabel}, ${yLabel}, ${percentLabel}'>   
                <border thikness='1' alpha='1' color='${color}{adjustColor:1.4}' pixelHinting='true'/>
                <fill color='333333' alpha='0.8'/>
                <format font='微软雅黑' size='12' color='${color}{adjustColor:1.3}' bold='false'/>
                <text substr='30'>
                    <effects>
                        <shadow color='${color}{adjustColor:0.3}' alpha='1' distance='1' blur='1' angle='90'/>
                    </effects>
                </text>
	        </tooltip>
	        
	        <legend label="${xLabel}, ${yLabel}" enable='true' position='right' hPadding='5' vPadding='0' vMargin='1' radius='5'>
	            <!--border color='B7B9BA' thikness='1' alpha='0.8'/-->
	            <fill color='EEEEEE' alpha='0'/>
	            <icon>
	                <states normal='legendIconStyle' hover='legendIconStyle'>
	                    <normal>
	                        <fill radioes='0,100,255' alpha='1,1,1' angle='45' 
	                        color='${color} {adjustColor : 1.1}, ${color}{adjustColor:1.1} ,${color}{adjustColor:0.9}'/>
	                    </normal>
	                    <hover>
	                        <fill radioes='0,150,255' alpha='1,1,1' angle='45' 
	                        color='${color} {adjustColor : 1.5}, ${color}{adjustColor:1.1} ,${color}{adjustColor:0.9}'/>
	                    </hover>
	                </states>
	            </icon>
	            
	            <label>
	                <format size='12' color='${color}' font='微软雅黑'/>
	                <text substr='30'>
	                </text>
	            </label>
	         </legend>
	        
	        <valueLabel enable='true' margin='0' padding='5' text='${valueLabel}'>
	            <format color='${color}{adjustColor:0.5}' size='14' font='微软雅黑'/>
	            <text>
	            	<effects>
						<shadow color='${color}{adjustColor:1.3}' alpha='1' distance='1' blur='1' angle='90'/>
					</effects>
	            </text>
	        </valueLabel>
	        
	        <!-- 基础定义 -->
	        <definition>
	            <style id='legendIconStyle' width='15' height='11'>
	                <border color='${color}{adjustColor:0.2}' thikness='1'/>
	            </style>
	        </definition>
	        
	        <colors>
	            <color value='0x00A8FF'/>
	            <color value='0xA97601'/>
	            <color value='0xFF99FF'/>
	            <color value='0x9BD124'/>
	            <color value='0x7967B9'/>
	            <color value='0xB3B3B3'/>
	            <color value='0xCCCC99'/>
	            <color value='0x0A9D0B'/>
	            <color value='0x0B57B2'/>
	            <color value='0xE64826'/>
	            <color value='0x9F21C8'/>
	            <color value='0x2F8A93'/>
	            <color value='0x4B5D84'/>
	        </colors>
	    </black>
	    
	    <white animation='true'>
	        <pieSeries>
	            <states>
	                <normal>
	                    <fill type='radial' radioes='0,230,255' alpha='0.8,0.8,0.8' angle='0'
	                    color='${color}{adjustColor:1},${color}{adjustColor:1.2},${color}{adjustColor:1}'/>
	                    <border thikness='1' alpha='0' color='${color}{adjustColor:1.3}' pixelHinting='false'/>
	                </normal>
	                <hover>
	                    <fill type='radial' radioes='0,220,255' alpha='1,1,1' angle='0'
	                    color='${color}{adjustColor:1},${color}{adjustColor:1.5},${color}{adjustColor:1}'/>
	                     <border thikness='2' alpha='0.8' color='${color}{adjustColor:1.3}' pixelHinting='false'/>
	                </hover>
	                <down>
	                    <fill type='radial' radioes='0,255' alpha='0.8,0.8' angle='0'
	                    color='${color}{adjustColor:1},${color}{adjustColor:0.8}'/>
	                </down>
	            </states>
	        </pieSeries>
	            
	        <series>
	            <pie labelField='label' valueField='value'/>
	        </series>
	        
	        <title hAlign='center' vMargin='0' vPadding='0'>
	            <format size='14' color='555555' bold='true' font='微软雅黑'/>
	            <text substr='30'>
	            </text>
	        </title>
	        <subTitle hAlign='center' vMargin='0' vPadding='0'>
	            <text substr='30'/>
	            <format size='12' color='555555' bold='false' font='微软雅黑'/>
	        </subTitle>
	        <chartBG paddingLeft='10' paddingRight='10' paddingTop='10' paddingBottom='10' radius='10' gutterRight='10'>
	            <border color='EEEEEE'/>
	            <fill color='FFFFFF'/>
	        </chartBG>
	        
	        <tooltip enable='true' locked='false' radius='0' padding='5' vMargin='10'
	        	text='${xLabel}, ${yLabel}, ${percentLabel}'>   
                <border thikness='1' alpha='1' color='${color}' pixelHinting='true'/>
                <fill color='FFFFFF' alpha='0.9'/>
                <cover>
                    <border color='FFFFFF'/>
                </cover>
                <format font='微软雅黑' size='12' color='${color}' bold='false'/>
                <text substr='30'>
            	</text>
	        </tooltip>
	        
	        <legend label="${xLabel}, ${yLabel}" enable='true' position='right' hPadding='5' vPadding='0' vMargin='1' radius='5'>
	            <!--border color='B7B9BA' thikness='1' alpha='0.8'/-->
	            <fill color='EEEEEE' alpha='0'/>
	            <icon>
	                <states normal='legendIconStyle' hover='legendIconStyle'>
	                    <normal>
	                        <fill radioes='0,100,255' alpha='1,1,1' angle='45' 
	                        color='${color} {adjustColor : 1.1}, ${color}{adjustColor:1.1} ,${color}{adjustColor:0.9}'/>
	                    </normal>
	                    <hover>
	                        <fill radioes='0,150,255' alpha='1,1,1' angle='45' 
	                        color='${color} {adjustColor : 1.5}, ${color}{adjustColor:1.1} ,${color}{adjustColor:0.9}'/>
	                    </hover>
	                </states>
	            </icon>
	            
	            <label>
	                <format size='12' color='${color}' font='微软雅黑'/>
	                <text substr='30'>
	                </text>
	            </label>
	         </legend>
	        
	        <valueLabel enable='true' margin='0' padding='5' text='${valueLabel}'>
	            <format color='${color}{adjustColor:0.5}' font='微软雅黑'/>
	            <text>
	            	<effects>
						<shadow color='${color}{adjustColor:1.4}' alpha='1' distance='1' blur='1' angle='90'/>
					</effects>
	            </text>
	        </valueLabel>
	        
	        <!-- 基础定义 -->
	        <definition>
	            
	            <style id='legendIconStyle' width='15' height='11'>
	                <border color='${color}{adjustColor:0.2}' thikness='1'/>
	            </style>
	 
	        </definition>
	        
	        <colors>
	            <color value='0x00A8FF'/>
	            <color value='0xA97601'/>
	            <color value='0xFF99FF'/>
	            <color value='0x9BD124'/>
	            <color value='0x7967B9'/>
	            <color value='0xB3B3B3'/>
	            <color value='0xCCCC99'/>
	            <color value='0x0A9D0B'/>
	            <color value='0x0B57B2'/>
	            <color value='0xE64826'/>
	            <color value='0x9F21C8'/>
	            <color value='0x2F8A93'/>
	            <color value='0x4B5D84'/>
	        </colors>
	    </white>
	</styles> 
</theme>    
