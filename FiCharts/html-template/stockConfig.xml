 <config precision="2" title="数据缩放" subTitle='鼠标滚动缩放，拖拽查看，背景双击全屏'>
   <!--tooltip>
   	<label><![CDATA[日期:${xValue}{formatDate:YYYYMMDD,YYYY-MM-DD}<br>数值:${yLabel}]]></label>
   </tooltip-->
   <axis>
     <x title="A" type="date" input="YYYYMMDD"/> 
     <y title="B" type="linear" baseAtZero='false'/> 
   </axis>
   <legend enable="false"/>
   <series>
     <line name="a" xField="label" yField="value"/> 
     <marker name="b" xField="label" yField="valueB"/> 
   </series>
   <zoom enable='true'/> 
   <zoomBar height='50' visible='true'>
	    <barBG>
	    	<border color="CCCCCC"/>
	        <fill color='EEEEEE'/> 
	    </barBG>
	    <chart>
	    	<border color='097DDA' thikness='1' alpha='1' pixelHinting='true'/>
            <fill color='097DDA' alpha='0.3'/>
            <xAxis output='YYYY' minUintSize='80' ifHideEdgeLabel='true'>
	            <tickMark color='E4E2E2' thikness='1' alpha='1'/>
	            <label margin='0'>
	                <format color='9F9F9F' font='微软雅黑'/>
	                <text value='${label}'>
	                	<effects>
	                        <shadow color='FFFFFF' alpha='1' distance='1' blur='1' angle='90'/>
	                    </effects>
	                </text>
	            </label>
	         </xAxis>
	         <yAxis/>
	    </chart>
	    <chartCover>
	    	<fill color='FFFFFF,555555' alpha='0,0.3' radioes='0,255' angle='90'/>
	    </chartCover>
	    <window>
	    	<states>
	    		<normal>
	    			<fill color='EEEEEE' alpha='0.1'/>
	    		</normal>
	    		<hover>
	    			<fill color='EEEEEE' alpha='0.1'/>
	    		</hover>
	    		<down>
	    			<fill color='EEEEEE' alpha='0.1'/>
	    		</down>
	    	</states>
	    </window>
   </zoomBar>
 </config>