<config ySuffix="K" bgVisible='true'>
    <axis>
        <x type="field" enable='false'/>
        <y id="a" type="linear" enable='false'>
            <label>
                <format color="${color}"/>
            </label>
            <title>
                <format color="${color}"/>
            </title>
        </y>
        <y id="b" type="linear" enable='false'>
            <label>
                <format color="${color}"/>
            </label>
            <title>
                <format color="${color}"/>
            </title>
        </y>
    </axis>
    <series>
        <area  xField="label" yAxis="a" yField="value" labelDisplay='none'/>
        <area  xField="label" yAxis="b" yField="valueA" labelDisplay='none'/>
    </series>
    <legend enable="true"/>
